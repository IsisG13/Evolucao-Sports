{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-expressions": "off",
        "no-unused-vars": "off",
        "react/jsx-no-target-blank": "off",
        "no-target-blank": "off",
        "react-in-jsx-scope": "off"
    }
}
